{
  "name": "ts_urbaniz",
  "version": "1.0.0",
  "description": "Reconstrução do site oficial da Urbaniz utilizando TypeScript, Node.js e Express",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "build": "webpack --mode production && tsc",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GalantaTech/ts_urbaniz.git"
  },
  "keywords": [
    "typescript",
    "express",
    "webpack",
    "urbaniz"
  ],
  "author": "GalantaTech",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "particlesjs": "^2.2.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "mini-css-extract-plugin": "^2.7.5",
    "nodemon": "^2.0.22",
    "rimraf": "^5.0.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4",
    "webpack": "^5.80.0",
    "webpack-cli": "^5.0.2"
  }
}